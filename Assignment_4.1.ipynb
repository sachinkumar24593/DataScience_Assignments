    "def filter_long_words(inlist,n):\n",
    "    outlist =[]\n",
    "    for x in inlist:\n",
    "        if len(x)>n:\n",
    "            outlist.append(x)    \n",
    "    return outlist\n",
    "inlist =['Sachin','kumar','yuvaraj','Saravanan']\n",
    "output = filter_long_words(inlist,4)\n",
    "print(output)"
